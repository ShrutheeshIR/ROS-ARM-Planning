<launch>
    <param name="bowl_sdf" textfile="$(find motoman_gazebo)/sdf/bowl/model.sdf" />
    <!-- <param name="t_brace_part_sdf" textfile="$(find motoman_gazebo)/sdf/t_brace_part/model.sdf" /> -->
    <param name="hammer_sdf" textfile="$(find motoman_gazebo)/sdf/hammer/model.sdf" />
    <param name="bottle_sdf" textfile="$(find motoman_gazebo)/sdf/bottle/model.sdf" />
    <!-- <param name="mouse_sdf" textfile="$(find motoman_gazebo)/sdf/mouse/model.sdf" /> -->
    <param name="torch_sdf" textfile="$(find motoman_gazebo)/sdf/torch/model.sdf" />
    <param name="mug_sdf" textfile="$(find motoman_gazebo)/sdf/mug/model.sdf" />

    <node name="$(anon spawnbowl)" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-sdf -param bowl_sdf -model bowl -x -0.12 -y 0.12 -z 1.075" />

    <node name="$(anon spawnhammer)" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-sdf -param hammer_sdf -model hammer -x 0.12 -y 0.12 -z 1.075" />


    <!-- <node name="$(anon spawn2)" pkg="gazebo_ros" type="spawn_model" output="screen"
