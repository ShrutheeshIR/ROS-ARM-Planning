    <!-- <node name="$(anon spawnbottle)" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-sdf -param bottle_sdf -model bottle -x -0.12 -y 0.10 -z 1.075" /> -->

    <!-- <node name="$(anon spawnmouse)" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-sdf -param mouse_sdf -model mouse -x 0.12 -y 0.10 -z 1.075" /> -->

    <node name="$(anon spawntorch)" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-sdf -param torch_sdf -model torch -x -0.12 -y -0.02 -z 1.075" />

    <node name="$(anon spawnmug)" pkg="gazebo_ros" type="spawn_model" output="screen"
        args="-sdf -param mug_sdf -model mug -x -0.07 -y 0.23 -z 1.075" />

