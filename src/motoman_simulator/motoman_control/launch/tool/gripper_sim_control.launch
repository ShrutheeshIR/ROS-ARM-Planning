<launch>

  <!-- Load joint controller configurations from YAML file to parameter server -->
  <rosparam file="$(find motoman_control)/config/tool/gripper_joint_position_control.yaml" command="load"/>

  <!-- load the controllers -->
  <node name="gripper_controller_spawner" pkg="controller_manager"
		type="spawner" output="screen" 
		args="gripper_controller_l gripper_controller_r"/>

</launch>
