<?xml version="1.0"?>
<package format="2">
  <name>motoman_control</name>
  <version>0.1.0</version>
  <description>The motoman_control package</description>

  <maintainer email="groadpg@gmail.com">RyodoTanaka</maintainer>
  <license>BSD</license>

  <url type="repository">http://github.com/ntl-ros-pkg/motoman_simulator</url>

  <author email="groadpg@gmail.com">Ryodo Tanaka</author>

  <buildtool_depend>catkin</buildtool_depend>

  <depend>roslaunch</depend>
  <depend>actionlib</depend>
  <depend>actionlib_msgs</depend>
  <depend>control_msgs</depend>
  <depend>sensor_msgs</depend>
